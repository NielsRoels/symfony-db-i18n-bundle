{
  "name": "creative/symfony-db-i18n-bundle",
  "type": "symfony-bundle",
  "description": "Allow store i18n-messages in database",
  "keywords": ["symfony", "i18n", "translations"],
  "license": "MIT",
  "version": "0.1",
  "authors": [
    {
      "name": "Andrew Zhdanovskih",
      "email": "azh@crtweb.ru"
    }
  ],
  "require": {
    "php": "^7.2",
    "doctrine/common": "^2.8",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/orm": "^2.6",
    "doctrine/persistence": "^1.0",
    "symfony/config": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/doctrine-bridge": "^4.1",
    "symfony/finder": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/polyfill-mbstring": "^1.7",
    "symfony/translation": "^4.1",
    "symfony/twig-bridge": "^4.1",
    "symfony/twig-bundle": "^4.1",
    "twig/extensions": "^1.5",
    "twig/twig": "^2.4",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "symfony/console": "^4.1",
    "symfony/css-selector": "^4.1",
    "symfony/dom-crawler": "^4.1",
    "symfony/phpunit-bridge": "^4.1",
    "symfony/var-dumper": "^4.1"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Creative\\DbI18nBundle\\": "src"
    }
  },
  "autoload-dev": {
    "Creative\\DbI18nBundle\\Tests\\": "tests/"
  }
}
